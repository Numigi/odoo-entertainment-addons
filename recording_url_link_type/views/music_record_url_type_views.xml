<?xml version="1.0" encoding="UTF-8" ?>
<odoo>

    <record id="music_record_url_type_view_search" model="ir.ui.view">
        <field name="name">music.record.url.type.view.search</field>
        <field name="model">music.record.url.type</field>
        <field name="arch" type="xml">
            <search>
                <field name="name"/>
            </search>
        </field>
    </record>

    <record id="music_record_url_type_view_tree" model="ir.ui.view">
        <field name="name">music.record.url.type.view.tree</field>
        <field name="model">music.record.url.type</field>
        <field name="arch" type="xml">
            <tree editable="top">
                <field name="name"/>
                <field name="active"/>
            </tree>
        </field>
    </record>

    <record id="music_record_url_type_action" model="ir.actions.act_window">
        <field name="name">Type of links</field>
        <field name="res_model">music.record.url.type</field>
        <field name="view_type">form</field>
        <field name="view_mode">tree</field>
    </record>

    <menuitem id="music_record_url_type_menu"
              name="Type of linksâ€‹"
              action="music_record_url_type_action"
              parent="recording.menu_config"
              sequence="3"
              groups="recording.group_manager"/>


</odoo>
