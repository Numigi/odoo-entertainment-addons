<?xml version="1.0" encoding="UTF-8"?>
<odoo noupdate="1">

    <record id="musical_artwork_distribution_sequence" model="ir.sequence">
        <field name="name">Musical Artwork Distribution</field>
        <field name="code">musical.artwork.distribution</field>
        <field name="prefix">REP/</field>
        <field name="padding">4</field>
        <field name="company_id" eval="False"/>
    </record>

    <record id="musical_artwork_sequence" model="ir.sequence">
        <field name="name">Musical Artwork</field>
        <field name="code">musical.artwork</field>
        <field name="prefix">EDTC</field>
        <field name="padding">8</field>
        <field name="company_id" eval="ref('base.main_company')" />
    </record>

</odoo>
